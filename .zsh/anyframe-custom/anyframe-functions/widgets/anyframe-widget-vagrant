local command
command=$(anyframe-source-vagrant-list-commands \
             | anyframe-selector-auto \
             | awk '{print $1}')

anyframe-source-vagrant-global-status \
  | anyframe-selector-auto \
  | awk '{print $1}' \
  | anyframe-action-execute vagrant ${command}

# Local Variables:
# mode: Shell-Script
# End:
# vim: ft=zsh
